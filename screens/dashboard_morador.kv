<MyReservasAreaModal@ModalView>:
    id: modal_reserva_area
    size_hint: 0.9, 0.9
    background_color: 1, 1, 1, 0
    background: ""

    MDCard:
        orientation: "vertical"
        padding: dp(20)
        spacing: dp(15)
        radius: [20]
        size_hint: None, None
        size: root.width * 1, root.height * 1
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        md_bg_color: 1, 1, 1, 0.95

        MDLabel:
            text: "Reservar Área Comum"
            halign: "left"
            font_style: "H5"
            theme_text_color: "Custom"
            text_color: 0.5, 0.3, 0.8, 1
            size_hint_y: None
            height: self.texture_size[1]

        ScrollView:
            MDBoxLayout:
                orientation: "vertical"
                spacing: dp(10)
                size_hint_y: None
                height: self.minimum_height

                MDTextField:
                    id: nome_area
                    hint_text: "Área Comum"
                    helper_text: "Ex: Salão de Festas"
                    helper_text_mode: "on_focus"

                MDTextField:
                    id: data_reserva
                    hint_text: "Data da Reserva"
                    readonly: True
                    on_focus: if self.focus: root.abrir_calendario('data_reserva')

                MDTextField:
                    id: data_fim_reserva
                    hint_text: "Data de Término"
                    readonly: True
                    on_focus: if self.focus: root.abrir_calendario('data_fim_reserva')

                MDTextField:
                    id: horario_reserva
                    hint_text: "Horário"
                    helper_text: "Ex: 18:00 às 22:00"
                    helper_text_mode: "on_focus"

                MDTextField:
                    id: observacoes
                    hint_text: "Observações (opcional)"
                    multiline: True
                    max_height: dp(100)

        BoxLayout:
            orientation: "horizontal"
            spacing: dp(10)
            size_hint_y: None
            height: dp(50)

            Widget:

            MDRaisedButton:
                text: "Cancelar"
                md_bg_color: 1, 1, 1, 1
                text_color: 0.5, 0.3, 0.8, 1
                on_release: root.dismiss()
                elevation: 0

            MDRaisedButton:
                text: "Reservar"
                md_bg_color: 0.5, 0.3, 0.8, 1
                text_color: 1, 1, 1, 1
                on_release: app.enviar_reserva()
                elevation: 0

            Widget:


<RegistrarOcorrenciaModal@ModalView>:
