<MyPerfilModal>:
    size_hint: 1, 1
    background_color: 1, 1, 1, 0
    background: ""

    MDBoxLayout:
        orientation: "vertical"
        padding: dp(30)
        spacing: dp(20)
        md_bg_color: 1, 1, 1, 1
        radius: [0, 0, 0, 0]

        MDBoxLayout:
            size_hint_y: None
            height: dp(180)
            padding: dp(10)
            spacing: dp(10)
            pos_hint: {"center_x": 0.5}
            orientation: "vertical"
            md_bg_color: 0.9, 0.9, 1, 1
            radius: [20]

            FitImage:
                source: app.resource_path("assets/perfil.png")
                size_hint: None, None
                size: dp(100), dp(100)
                pos_hint: {"center_x": 0.5}
                radius: [50]

            MDLabel:
                id: nome_usuario
                text: "Nome do Usuário"
                font_style: "H5"
                halign: "center"
                theme_text_color: "Custom"
                text_color: 0.4, 0.2, 0.8, 1

        MDCard:
            padding: dp(20)
            spacing: dp(10)
            orientation: "vertical"
            radius: [15]
            md_bg_color: 0.96, 0.96, 0.96, 1
            size_hint_y: None
            height: dp(220)

            MDLabel:
                text: "Informações do Perfil"
                font_style: "Subtitle1"
                text_color: 0.4, 0.2, 0.8, 1
                theme_text_color: "Custom"
                halign: "left"

            MDSeparator:

            MDLabel:
                id: email_usuario
                text: "E-mail: exemplo@email.com"
                halign: "left"
                theme_text_color: "Secondary"

            MDLabel:
                id: tipo_usuario
                text: "Tipo: Morador"
                halign: "left"
                theme_text_color: "Secondary"

            MDLabel:
                id: apartamento_usuario
                text: "Apartamento: 203B"
                halign: "left"
                theme_text_color: "Secondary"
                opacity: 1 

        Widget:

        MDRaisedButton:
            text: "Fechar"
            md_bg_color: 0.5, 0.3, 0.8, 1
            text_color: 1, 1, 1, 1
            pos_hint: {"center_x": 0.5}
            on_release: root.dismiss()
            size_hint_x: 0.5
            elevation: 0


