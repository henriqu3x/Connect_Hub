<DashboardMoradorScreen>:
    name: 'morador_dashboard'

    BoxLayout:
        orientation: 'horizontal'

        BoxLayout:
            orientation: "vertical"
            size_hint: 0.2, 1.05
            padding: 0
            spacing: 10
            canvas.before:
                Color:
                    rgba: 1, 1, 1, 1 
                Rectangle:
                    pos: self.pos
                    size: self.size
                Color:
                    rgba: 0.5, 0.3, 0.8, 1 
                Line:
                    width: 2 
                    points: [self.right, self.y, self.right, self.top]

            BoxLayout:
                orientation: "horizontal"
                size_hint_y: None  
                height: dp(200) 

                Image:
                    source: app.resource_path("assets/Logo-roxa-copia.png")
                    size_hint_x: None
                    width: self.height 
                    allow_stretch: True
                    keep_ratio: True

                MDIconButton:
                    icon: "logout"
                    theme_text_color: "Custom"
                    text_color: 0.5, 0.3, 0.8, 1
                    pos_hint: {"center_y": 0.5}
                    on_release: app.logout() 

            ScrollView:
                do_scroll_x: False

                BoxLayout:
                    orientation: "vertical"
                    size_hint_y: None
                    height: self.minimum_height
                    spacing: 15
                    padding: [23,0,0,0]

                    BoxLayout:
                        orientation: "horizontal"
